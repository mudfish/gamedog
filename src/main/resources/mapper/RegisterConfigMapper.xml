<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="registerConfigMapper">
    <resultMap id="registerConfigMap" type="com.laoxu.gamedog.model.RegisterConfig">
        <result property="goldCoin" column="GOLD_COIN" />
        <result property="silverCoin" column="SILVER_COIN" />
        <result property="limitHour" column="LIMIT_HOUR" />
        <result property="limitNum" column="LIMIT_NUM" />
    </resultMap>

    <select id="load" resultMap="registerConfigMap">
        select gold_coin,
               silver_coin,
               limit_hour,
               limit_num
        from a_code
    </select>

</mapper>